        <script type="text/javascript">
   
   fetch('/data')
  .then(response => response.json())
  .then(data => {
    var labels = data.map(item => item.indice);
    console.log(labels);
    var values = data.map(item => item.nature);
    console.log(values);

    console.log('values:::', values);
    var chartData = {
      labels: labels,
      datasets: [
        {
          label: "My First dataset",
          backgroundColor: "rgba(220,220,220,0.2)",
          borderColor: "rgba(220,220,220,1)",
          pointBackgroundColor: "rgba(220,220,220,1)",
          pointBorderColor: "#fff",
          pointHoverBackgroundColor: "red",
          pointHoverBorderColor: "rgba(220,220,220,1)",
          data: values,
        }
      ]
    };

    var pieChartData = labels.map((label, index) => ({
      value: values[index],
      backgroundColor: getRandomColor(),
      hoverBackgroundColor: getRandomColor(),
      label: label
    }));
    

  
    var ctxl = $("#lineChartDemo").get(0).getContext("2d");
      var lineChart = new Chart(ctxl).Line(chartData);

    var ctxb = $("#barChartDemo").get(0).getContext("2d");
      var barChart = new Chart(ctxb).Bar(chartData);

      var pieChartData = labels.map((label, index) => ({
  value: values[index],
  backgroundColor: getRandomColor(),
  hoverBackgroundColor: getRandomColor(),
  label: label
}));

var pieChartCtx = document.getElementById("pieChartDemo").getContext("2d");
var pieChart = new Chart(pieChartCtx, {
  type: 'pie',
  data: {
    datasets: [{
      data: values,
      backgroundColor: pieChartData.map(item => item.backgroundColor),
      hoverBackgroundColor: pieChartData.map(item => item.hoverBackgroundColor),
    }],
    labels: labels,
  },
});


    var doughnutChartCtx = document.getElementById("doughnutChartDemo").getContext("2d");
    var doughnutChart = new Chart(doughnutChartCtx, {
      type: 'doughnut',
      data: {
        datasets: [{
          data: values,
          backgroundColor: pieChartData.map(item => item.backgroundColor),
          hoverBackgroundColor: pieChartData.map(item => item.hoverBackgroundColor),
        }],
        labels: labels,
      },
    });
  });

function getRandomColor() {
  var letters = "0123456789ABCDEF";
  var color = "#";
  for (var i = 0; i < 6; i++) {
    color += letters[Math.floor(Math.random() * 16)];
  }
  return color;
}
      </script>
